{
    "name": "真白萌",
    "url": "masiro.me",
    "version": 100,
    "search": {
        "url": "https://masiro.me/admin/loadMoreNovel?page=${page}&keywoed=${key}",
        "charset": "utf-8",
        "list": "$.novels",
        "name": " $.title",
        "author": "$.author",
        "summary": "$.brief",
        "cover": "&.cover_img",
        "detail": "div.layui-card-body > a@attr->href"
    },
    "detail": {
        "name": "div.novel-title",
        "author": "div.n-detail > div.author > a",
        "cover": "div.mailbox > a > img@attr->src",
        "summary": "div.brief",
        "status": "div.n-detail > div.n-status",
        "update": "div.n-detail > div.n-update > a",
        "lastChapter": "div.n-detail > div.n-update > span.s-font",
        "catalog": ""
    },
    "catalog": {
        "list": "div.chapter-content > ul",
        "booklet": {
            "name": "li.chapter-box > b",
            "list": "li > ul.epidode-ul"
        },
        "name": "a > span",
        "chapter": "a@attr->href"
    },
    "chapter": {
        "content": "div.row > div > div > div > p"
    },
    "auth": {
        "login": "https://masiro.me/admin/auth/login",
        "cookie": "masiro.me",
        "verify": "https://masiro.me/admin/",
        "logged": "div.pull-right > a@equal->登出"
    },
    "rank": [
        {
            "title": "小说",
            "page": 1,
            "size": 20,
            "url": "https://masiro.me/admin/novels?ori=${key}",
            "categories": [
                {
                    "value": "全部",
                    "key": ""
                },
                {
                    "value": "日轻",
                    "key": "0"
                },
                {
                    "value": "原创",
                    "key": "0"
                }
            ]
        }
    ]
}
