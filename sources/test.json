{
    "name": "真白萌",
    "url": "masiro.me",
    "version": 100,
    "search": {
        "url": "https://masiro.me/admin/loadMoreNovels?page=${page}&keyword=${key}",
        "charset": "utf-8",
        "list": "$.novels",
        "name": "$.title",
        "author": "$.author",
        "summary": "$.brief",
        "cover": "$.cover_img",
        "detail": "https://masiro.me/admin/novelView?novel_id=${$,id}"
    },
    "detail": {
        "name": "div.novel-title",
        "author": "div.n-detail > div.author > a",
        "cover": "div.with-border > span > div > a > img@attr->src",
        "summary": "div.brief",
        "status": "div.n-detail > div.n-status",
        "update": "div.n-detail > div.n-update > span.s-font",
        "words": "div.n-detail > div.n-chapters",
        "lastChapter": "div.n-detail > div.n-update > a.n-wa",
        "catalog": ""
    },
    "catalog": {
        "list": "ul.episode-ul>a",
        "booklet": {
            "list": "div.chapter-content > ul.chapter-ul > li.chapter-box > span",
            "name": "div.chapter-content > ul.chapter-ul > li.chapter-box > b"
        },
        "name": "li",
        "chapter": "a@attr->href"
    },
    "chapter": {
        "content": "div.row > div.col-md-12 > div.read-box > div.nvl-content"
    },
    "auth": {
        "login": "https://masiro.me/admin/auth/login",
        "cookie": "masiro.me",
        "verify": "https://masiro.me/admin/",
        "logged": "div.pull-right > a@equal->登出"
    },
    "rank": [
        {
            "title": "小说",
            "page": 1,
            "size": 30,
            "url": "https://masiro.me/admin/loadMoreNovels?page=${page}&status=1&order=2&ori=${key}",
            "categories": [
                {
                    "value": "全部",
                    "key": ""
                },
                {
                    "value": "日轻",
                    "key": "0"
                },
                {
                    "value": "原创",
                    "key": "1"
                }
            ]
        },
        {
            "title": "最近更新",
            "page": 1,
            "size": 30,
            "url": "https://masiro.me/admin/loadMoreNovels?page=${page}&order=1"
        },
        {
            "title": "收藏",
            "page": 1,
            "size": 30,
            "url": "https://masiro.me/admin/loadMoreNovels?page=${page}&collection=1"
        }
    ]
}
